{
  "name": "roxoepos",
  "private": true,
  "license": "UNLICENSED",
  "version": "0.5.3",
  "type": "module",
  "description": "",
  "author": {
    "name": "Batin",
    "email": "batin@cretique.net"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "build:win": "tsc && vite build && electron-builder --win --x64",
    "build:mac": "tsc && vite build && electron-builder --mac --x64",
    "build:all": "tsc && vite build && electron-builder -mw --x64",
    "publish": "tsc && vite build && electron-builder --publish always --x64",
    "publish:win": "tsc && vite build && electron-builder --win --publish always --x64",
    "publish:mac": "tsc && vite build && electron-builder --mac --publish always --x64",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run --coverage",
    "test:integration": "vitest run \"src/integration/**/*.test.ts\"",
    "test:performance": "vitest run \"src/performance/**/*.test.ts\"",
    "test:critical": "vitest run --coverage && node scripts/check-coverage.js",
    "preview": "vite preview",
    "e2e": "playwright test",
    "test:e2e": "playwright test",
    "e2e:ci": "playwright test --reporter=dot",
    "prepare": "husky",
    "validate:schemas": "ajv compile --spec=draft7 -c ajv-formats -s \"../docs/schemas/**/*.json\"",
    "validate:ipc:update-status": "ajv validate --spec=draft7 -c ajv-formats -s \"../docs/schemas/ipc/update-status.schema.json\" -d \"../docs/samples/ipc/update-status.sample.json\"",
    "validate:samples": "node scripts/validate-samples.js",
    "validate:all": "npm run validate:schemas && npm run validate:samples"
  },
  "dependencies": {
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@sentry/electron": "^5.1.0",
    "@shadcn/ui": "^0.0.4",
    "@types/react-datepicker": "^6.2.0",
    "axios": "^1.8.1",
    "better-sqlite3": "^11.8.1",
    "blob-stream": "^0.1.3",
    "canvg": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.2.4",
    "dotenv": "^16.4.7",
    "electron-log": "^5.3.1",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.3.9",
    "exceljs": "^4.4.0",
    "framer-motion": "^12.6.1",
    "idb": "^8.0.1",
    "lucide-react": "^0.477.0",
    "lz-string": "^1.5.0",
    "node-machine-id": "^1.1.12",
    "papaparse": "^5.5.1",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.16.0",
    "react": "^18.2.0",
    "react-datepicker": "^8.0.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.1.3",
    "react-window": "^1.8.9",
    "recharts": "^2.15.0",
    "roxoepos-root": "file:..",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@playwright/test": "^1.48.0",
    "@radix-ui/react-switch": "^1.0.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.12",
    "@types/blob-stream": "^0.1.33",
    "@types/crypto-js": "^4.2.2",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^22.13.1",
    "@types/papaparse": "^5.3.15",
    "@types/pdfkit": "^0.13.9",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^3.2.4",
    "ajv": "^8.17.1",
    "ajv-cli": "^5.0.0",
    "ajv-formats": "^3.0.1",
    "autoprefixer": "^10.4.20",
    "electron": "^35.0.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "fake-indexeddb": "^5.0.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "javascript-obfuscator": "^4.1.1",
    "jsdom": "^26.1.0",
    "jsesc": "^3.1.0",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "main": "dist-electron/main.js",
  "build": {
    "appId": "com.roxoepos.app",
    "productName": "RoxoePOS",
    "copyright": "Copyright Â© 2025 Roxoe",
    "directories": {
      "output": "release/${version}"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "emirbatin",
        "repo": "RoxoePOS",
        "releaseType": "release",
        "private": true,
        "token": "${env.GH_TOKEN}"
      }
    ],
    "electronDownload": {
      "arch": "x64"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/icon.icns",
      "category": "public.app-category.business",
      "artifactName": "RoxoePOS-Mac-${version}.${ext}"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "differentialPackage": true,
      "installerIcon": "public/icon.ico",
      "uninstallerIcon": "public/icon.ico",
      "include": "installer.nsh",
      "deleteAppDataOnUninstall": false,
      "runAfterFinish": true,
      "artifactName": "RoxoePOS-Setup-${version}.${ext}",
      "allowElevation": true
    },
    "portable": {
      "artifactName": "RoxoePOS-Portable-${version}.${ext}"
    },
    "dmg": {
      "artifactName": "RoxoePOS-Installer-${version}.${ext}"
    },
    "asarUnpack": [
      "node_modules/better-sqlite3"
    ],
    "compression": "normal"
  }
}
